{
  "name": "doterra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run type-check && npm run lint:fix && npm run format",
    "clean": "rm -rf .next dist out",
    "scrape:new": "tsx scripts/new-scraper.ts",
    "scrape:v2": "tsx scripts/new-scraper-v2.ts",
    "scrape:working": "tsx scripts/working-scraper.ts",
    "scrape:single-oils": "tsx scripts/category-scraper.ts single-oils",
    "scrape:blends": "tsx scripts/category-scraper.ts blends",
    "scrape:skincare": "tsx scripts/category-scraper.ts skincare",
    "scrape:wellness": "tsx scripts/category-scraper.ts wellness",
    "scrape:onguard": "tsx scripts/scrape-onguard.ts",
    "products:merge": "tsx scripts/merge-products.ts",
    "products:stats": "tsx -e \"import { getProductStats } from './src/data/products'; console.log(getProductStats())\"",
    "cleanup": "tsx scripts/cleanup.ts",
    "cleanup:cache": "tsx scripts/cleanup.ts --cache-only",
    "cleanup:deps": "tsx scripts/cleanup.ts --refresh-deps"
  },
  "dependencies": {
    "browser-image-compression": "^2.0.2",
    "fuse.js": "^7.1.0",
    "next": "15.5.3",
    "playwright": "^1.55.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.3",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
