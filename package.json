{
  "name": "doterra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run type-check && npm run lint:fix && npm run format",
    "clean": "rm -rf .next dist out",
    "scrape:doterra": "tsx scripts/scrape-doterra.ts",
    "scrape:doterra-advanced": "tsx scripts/scrape-doterra-advanced.ts",
    "scrape:product-details": "tsx scripts/scrape-product-details.ts",
    "scrape:product-details:test": "tsx scripts/scrape-product-details.ts --test",
    "update:product-urls": "tsx scripts/update-product-urls.ts",
    "debug:doterra": "tsx scripts/debug-doterra-site.ts",
    "analyze:doterra": "tsx scripts/analyze-doterra-elements.ts",
    "generate:sample-data": "tsx scripts/generate-sample-data.ts"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "browser-image-compression": "^2.0.2",
    "cheerio": "^1.1.2",
    "next": "15.5.3",
    "playwright": "^1.55.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.3",
    "tsx": "^4.20.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
